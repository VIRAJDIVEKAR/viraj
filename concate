#include <iostream>
#include<string.h>
using namespace std;

int main()
{
    string s1="hello ";
    string s2="world";
    char s3[20];
    int j=0;
    for(int i=0;s1[i]!='\0';i++)
    {
        s3[j]=s1[i];
        j++;
    }
    for(int i=0;s2[i]!='\0';i++)
    {
        s3[j]=s2[i];
        j++;
    }
    
    cout<<s3;
    return 0;
}
